{
  "name": "blank",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite --emptyOutDir build",
    "preview": "vite preview",
    "predeploy": "git checkout main && npm run build -- --base=/blank/",
    "deploy": "gh-pages -d dist && npm run copy-html",
    "copy-html": "copyfiles -u 1 'src/**/*.html' dist"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "fast-glob": "^3.3.2",
    "gh-pages": "^6.1.0",
    "jquery": "^3.7.1",
    "sass": "^1.69.5",
    "vite": "^5.0.0",
    "vite-plugin-html": "^3.2.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "dependencies": {
    "bootstrap": "^5.3.2"
  }
}
